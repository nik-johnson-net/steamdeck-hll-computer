{
	"$schema": "https://schemas.elgato.com/streamdeck/plugins/manifest.json",
	"Name": "Hell Let Loose Artillery Calculator",
	"Version": "1.0.0.0",
	"Author": "Nik Johnson",
	"Actions": [
		{
			"Controllers": [
				"Keypad"
			],
			"Icon": "imgs/actions/new-message",
			"Name": "Number Input",
			"PropertyInspectorPath": "ui/number-input.html",
			"States": [
				{
					"Image": "imgs/actions/black",
					"TitleAlignment": "middle",
					"FontSize": 10,
					"FontFamily": "Courier",
					"FontStyle": "Bold"
				}
			],
			"Tooltip": "This action enters the number into the distance input",
			"UUID": "net.nik-johnson.hll-computer.number-input"
		},
		{
			"Controllers": [
				"Keypad"
			],
			"Icon": "imgs/actions/login",
			"Name": "Input Buffer",
			"States": [
				{
					"Image": "imgs/actions/black",
					"TitleAlignment": "middle",
					"FontSize": 18,
					"FontFamily": "Courier",
					"FontStyle": "Bold"
				}
			],
			"Tooltip": "This action shows the current input buffer",
			"UUID": "net.nik-johnson.hll-computer.input-buffer"
		},
		{
			"Controllers": [
				"Keypad"
			],
			"Icon": "imgs/actions/log-out",
			"Name": "Computed Value",
			"PropertyInspectorPath": "ui/computed-value.html",
			"States": [
				{
					"Image": "imgs/actions/black",
					"TitleAlignment": "middle",
					"FontSize": 12,
					"FontFamily": "Courier",
					"FontStyle": "Bold"
				}
			],
			"Tooltip": "This action shows a past computed value",
			"UUID": "net.nik-johnson.hll-computer.computed-value"
		},
		{
			"Controllers": [
				"Keypad"
			],
			"Icon": "imgs/actions/cycle",
			"Name": "Mode Change",
			"States": [
				{
					"Image": "imgs/actions/black",
					"Title": "Mode:\nAmerican",
					"TitleAlignment": "middle",
					"FontSize": 10,
					"FontFamily": "Courier",
					"FontStyle": "Bold"
				},
				{
					"Image": "imgs/actions/black",
					"Title": "Mode:\nGerman",
					"TitleAlignment": "middle",
					"FontSize": 10,
					"FontFamily": "Courier",
					"FontStyle": "Bold"
				},
				{
					"Image": "imgs/actions/black",
					"Title": "Mode:\nRussian",
					"TitleAlignment": "middle",
					"FontSize": 10,
					"FontFamily": "Courier",
					"FontStyle": "Bold"
				},
				{
					"Image": "imgs/actions/black",
					"Title": "Mode:\nBritish",
					"TitleAlignment": "middle",
					"FontSize": 10,
					"FontFamily": "Courier",
					"FontStyle": "Bold"
				}
			],
			"Tooltip": "This action changes the computer for different factions",
			"UUID": "net.nik-johnson.hll-computer.mode-change"
		}
	],
	"Category": "Hell Let Loose Artillery Calculator",
	"CategoryIcon": "imgs/plugin/category-icon",
	"CodePath": "bin/plugin.js",
	"Description": "Turn your stream deck into a tool to zero in artillery guns in Hell Let Loose!",
	"Icon": "imgs/plugin/marketplace",
	"SDKVersion": 2,
	"Software": {
		"MinimumVersion": "6.4"
	},
	"OS": [
		{
			"Platform": "mac",
			"MinimumVersion": "10.15"
		},
		{
			"Platform": "windows",
			"MinimumVersion": "10"
		}
	],
	"Profiles": [
		{
			"Name": "profiles/hll-computer-0",
			"DeviceType": 0,
			"Readonly": false
		},
		{
			"Name": "profiles/hll-computer-xl",
			"DeviceType": 2,
			"Readonly": false
		}
	],
	"Nodejs": {
		"Version": "20",
		"Debug": "enabled"
	},
	"URL": "https://github.com/nik-johnson-net/streamdeck-hll-computer",
	"UUID": "net.nik-johnson.hll-computer"
}
